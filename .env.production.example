# Production Environment Variables for Railway Backend
# Copy this to .env.production and replace with your actual Railway URL

# Backend API URL - REQUIRED for production
# Replace with your Railway URL (e.g., https://your-app-name.up.railway.app)
REACT_APP_API_URL=https://your-railway-url.up.railway.app

# Firebase Configuration (same as development)
REACT_APP_FIREBASE_API_KEY=your_api_key_here
REACT_APP_FIREBASE_AUTH_DOMAIN=your_project.firebaseapp.com
REACT_APP_FIREBASE_PROJECT_ID=your_project_id
REACT_APP_FIREBASE_STORAGE_BUCKET=your_project.appspot.com
REACT_APP_FIREBASE_MESSAGING_SENDER_ID=your_sender_id
REACT_APP_FIREBASE_APP_ID=your_app_id
